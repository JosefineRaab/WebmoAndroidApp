<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.josieraab.webmoandroidapp">

    <application
        android:name=".WebMoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login"
            android:label="Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ShowMealPlan"
            android:label="@string/title_activity_meal_plan">

        </activity>
        <activity
            android:name=".ShowMeal"
            android:label="@string/title_activity_show_meal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Navigate"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".EditMeal" />
        <activity
            android:name=".EditWeek"
            android:label="@string/title_activity_edit_week"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>